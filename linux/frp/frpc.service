[Unit]
Description=Frp Server Service
After=network.target
Wants=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/home/bgzo/opt/frp/frp_0.65.0_linux_amd64
ExecStart=/home/bgzo/opt/frp/frp_0.65.0_linux_amd64/frpc -c /home/bgzo/opt/frp/frp_0.65.0_linux_amd64/frpc.toml
ExecReload=/bin/kill -HUP $MAINPID
Restart=on-failure
RestartSec=5s
LimitNOFILE=1048576

[Install]
WantedBy=multi-user.target
